cmd + ctrl - n : nightlight toggle

cmd + ctrl + shift - h : yabai -m window --swap west
cmd + ctrl + shift - j : yabai -m window --swap north
cmd + ctrl + shift - k : yabai -m window --swap south
cmd + ctrl + shift - l : yabai -m window --swap east

cmd + ctrl - h : yabai -m window --focus west
cmd + ctrl - j : yabai -m window --focus north
cmd + ctrl - k : yabai -m window --focus south
cmd + ctrl - l : yabai -m window --focus east

shift + alt - space : yabai -m space --focus recent
shift + ctrl - 1 : yabai -m space --focus 1
shift + ctrl - 2 : yabai -m space --focus 2
shift + ctrl - 3 : yabai -m space --focus 3
shift + ctrl - 4 : yabai -m space --focus 4
shift + ctrl - 5 : yabai -m space --focus 5
shift + ctrl - 6 : yabai -m space --focus 6
shift + ctrl - 7 : yabai -m space --focus 7
shift + ctrl - 8 : yabai -m space --focus 8
shift + ctrl - 9 : yabai -m space --focus 9

shift + ctrl - a : yabai -m space --move prev
shift + ctrl - d : yabai -m space --move next

ctrl - t : yabai -m window --toggle float;\
           yabai -m window --grid 4:4:1:1:2:2

shift + alt - up : yabai -m window --grid 1:1:0:0:1:1

ctrl - p : yabai -m window --toggle sticky;\
           yabai -m window --toggle topmost;\
           yabai -m window --toggle pip
